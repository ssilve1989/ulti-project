{
  "name": "ulti-project",
  "packageManager": "pnpm@10.29.3",
  "version": "1.40.2",
  "description": "Ulti-Project Discord Bot",
  "author": "Aeo <aeo.games14@gmail.com>",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "build": "rimraf dist && tsgo -p tsconfig.build.json",
    "check": "biome check src",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docker:build": "docker build . -t ulti-project-bot:latest",
    "format": "biome format . --write",
    "g:slash-command": "hygen slash-command new",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "lint": "biome lint --diagnostic-level=error src",
    "lint:ci": "biome ci --diagnostic-level=error src",
    "prepare": "node scripts/prepare.js",
    "start:sentry": "NODE_OPTIONS=\"--import=./instrumentation.mjs\" pnpm start",
    "start": "dotenvx run -f .env -f .env.development -- node dist/main",
    "test": "vitest",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsgo --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@googleapis/sheets": "^13.0.1",
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/cqrs": "^11.0.3",
    "@sentry/cli": "^3.1.0",
    "@sentry/nestjs": "^10.38.0",
    "@sentry/profiling-node": "^10.38.0",
    "cron": "^4.4.0",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.1",
    "firebase-admin": "^13.6.1",
    "google-auth-library": "^10.5.0",
    "graphql-request": "^7.4.0",
    "nestjs-pino": "^4.5.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "title-case": "^4.3.2",
    "ts-pattern": "^5.9.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@commitlint/cz-commitlint": "^20.4.1",
    "@commitlint/prompt-cli": "^20.4.1",
    "@flydotio/dockerfile": "^0.7.10",
    "@golevelup/ts-vitest": "^0.5.2",
    "@graphql-codegen/cli": "6.1.1",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.4.0",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@types/node": "^25.2.3",
    "@typescript/native-preview": "7.0.0-dev.20260131.1",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.18"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@firebase/util",
      "@nestjs/core",
      "@sentry-internal/node-cpu-profiler",
      "@sentry/cli",
      "@swc/core",
      "esbuild",
      "nestjs-pino"
    ],
    "ignoredBuiltDependencies": [
      "protobufjs"
    ]
  }
}
