{
  "name": "ulti-project",
  "packageManager": "pnpm@9.15.5+sha512.845196026aab1cc3f098a0474b64dfbab2afe7a1b4e91dd86895d8e4aa32a7a6d03049e2d0ad770bbe4de023a7122fb68c1a1d6e0d033c7076085f9d5d4800d4",
  "version": "1.28.0",
  "description": "Ulti-Project Discord Bot",
  "author": "",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "check": "biome check src",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docker:build": "docker build . -t ulti-project-bot:latest",
    "format": "biome format . --write",
    "g:slash-command": "hygen slash-command new",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "lint": "biome lint --diagnostic-level=error src",
    "lint:ci": "biome ci src",
    "prepare": "node scripts/prepare.js",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start": "node dist/main",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist"
  },
  "dependencies": {
    "@googleapis/sheets": "^9.3.1",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.9",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.9",
    "@nestjs/cqrs": "^11.0.1",
    "@sentry/cli": "^2.41.1",
    "@sentry/node": "^9.0.0",
    "@sentry/profiling-node": "^9.0.0",
    "cache-manager": "^6.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.5.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.18.0",
    "firebase-admin": "^13.1.0",
    "google-auth-library": "^9.15.1",
    "graphql-request": "^7.1.0",
    "nestjs-pino": "^4.3.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "title-case": "^4.3.2",
    "ts-pattern": "^5.6.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@commitlint/prompt-cli": "^19.7.1",
    "@flydotio/dockerfile": "^0.7.8",
    "@golevelup/ts-vitest": "^0.5.2",
    "@graphql-codegen/cli": "5.0.4",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "4.1.3",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.9",
    "@types/node": "^22.13.4",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "inquirer": "^9.3.7",
    "jest-extended": "^4.0.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "typescript": "^5.7.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.5"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
