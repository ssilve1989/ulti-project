{
  "name": "ulti-project",
  "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca",
  "version": "1.17.4",
  "description": "Ulti-Project Discord Bot",
  "author": "",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && nest build",
    "check": "biome check src",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docker:build": "docker build . -t ulti-project-bot:latest",
    "format": "biome format . --write",
    "g:slash-command": "hygen slash-command new",
    "lint": "biome lint --diagnostic-level=error src",
    "lint:ci": "biome ci src",
    "prepare": "node scripts/prepare.js",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start": "node dist/main",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist"
  },
  "dependencies": {
    "@googleapis/sheets": "^9.3.1",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/cqrs": "^10.2.7",
    "@sentry/cli": "^2.38.0",
    "@sentry/node": "^8.35.0",
    "@sentry/profiling-node": "^8.35.0",
    "cache-manager": "^5.7.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.13",
    "discord.js": "^14.16.3",
    "firebase-admin": "^12.7.0",
    "google-auth-library": "^9.14.2",
    "nestjs-pino": "^4.1.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "title-case": "^4.3.2",
    "ts-pattern": "^5.5.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@flydotio/dockerfile": "^0.5.9",
    "@golevelup/ts-vitest": "^0.5.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@types/node": "^22.8.1",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/ui": "^2.1.4",
    "commitizen": "^4.3.1",
    "husky": "^9.1.6",
    "hygen": "^6.2.11",
    "inquirer": "^9.3.7",
    "jest-extended": "^4.0.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "typescript": "^5.6.3",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.1.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
