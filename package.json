    "hygen": "^6.2.11",
    "inquirer": "^9.3.2",
    "jest-extended": "^4.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "unplugin-swc": "^1.5.1",
    "vitest": "^1.6.0"
