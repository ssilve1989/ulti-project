{
  "name": "ulti-project",
  "version": "1.40.2",
  "packageManager": "bun@1.3.5",
  "description": "Ulti-Project Discord Bot",
  "author": "Aeo <aeo.games14@gmail.com>",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "check": "biome check",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docker:build": "docker build . -t ulti-project-bot:bun",
    "format": "biome format --write",
    "g:slash-command": "hygen slash-command new",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "lint": "biome lint",
    "lint:ci": "biome ci --diagnostic-level=error",
    "prepare": "husky",
    "start:dev": "bun run --watch ./src/main.ts",
    "start": "dotenvx run -f .env -f .env.development -- bun run ./src/main.ts",
    "test": "vitest",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "@googleapis/sheets": "^13.0.1",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/cqrs": "^11.0.3",
    "@sentry/cli": "^3.0.0",
    "@sentry/nestjs": "^10.30.0",
    "cron": "^4.4.0",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.1",
    "firebase-admin": "^13.6.0",
    "google-auth-library": "^10.5.0",
    "graphql-request": "^7.3.5",
    "nestjs-pino": "^4.5.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "title-case": "^4.3.2",
    "ts-pattern": "^5.9.0",
    "zod": "^4.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/cz-commitlint": "^20.2.0",
    "@commitlint/prompt-cli": "^20.2.0",
    "@flydotio/dockerfile": "^0.7.10",
    "@golevelup/ts-vitest": "^2.1.1",
    "@graphql-codegen/cli": "6.1.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "5.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.3.0",
    "@nestjs/testing": "^11.1.9",
    "@types/bun": "^1.3.5",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.15"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@nestjs/core",
    "@sentry/cli",
    "@swc/core",
    "nestjs-pino",
    "protobufjs"
  ]
}
