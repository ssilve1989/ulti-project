{
  "name": "ulti-project",
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "version": "1.40.1",
  "description": "Ulti-Project Discord Bot",
  "author": "Aeo <aeo.games14@gmail.com>",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines": {
    "node": "^24.5.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "check": "biome check src",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docker:build": "docker build . -t ulti-project-bot:latest",
    "format": "biome format . --write",
    "g:slash-command": "hygen slash-command new",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "lint": "biome lint --diagnostic-level=error src",
    "lint:ci": "biome ci --diagnostic-level=error src",
    "prepare": "node scripts/prepare.js",
    "start:sentry": "NODE_OPTIONS=\"--import=./instrumentation.mjs\" pnpm start",
    "start": "dotenvx run -f .env -f .env.development -- node dist/main",
    "test": "vitest",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@googleapis/sheets": "^12.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/cqrs": "^11.0.3",
    "@sentry/cli": "^2.53.0",
    "@sentry/nestjs": "^10.11.0",
    "@sentry/profiling-node": "^10.11.0",
    "cron": "^4.3.3",
    "dayjs": "^1.11.18",
    "discord.js": "^14.22.1",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.3.0",
    "graphql-request": "^7.2.0",
    "nestjs-pino": "^4.4.1",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "title-case": "^4.3.2",
    "ts-pattern": "^5.8.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@commitlint/prompt-cli": "^20.1.0",
    "@flydotio/dockerfile": "^0.7.10",
    "@golevelup/ts-vitest": "^0.5.2",
    "@graphql-codegen/cli": "6.0.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/node": "^24.4.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.7",
    "vitest": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@firebase/util",
      "@nestjs/core",
      "@sentry-internal/node-cpu-profiler",
      "@sentry/cli",
      "@swc/core",
      "esbuild",
      "nestjs-pino"
    ],
    "ignoredBuiltDependencies": [
      "protobufjs"
    ]
  }
}
