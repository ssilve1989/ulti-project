{
  "name": "ulti-project",
  "packageManager": "pnpm@8.11.0",
  "version": "1.1.0",
  "description": "Ulti-Project Discord Bot",
  "author": "",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "check": "biome check src",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "format": "biome format . --write",
    "lint": "biome lint --diagnostic-level=error src",
    "lint:ci": "biome ci --diagnostic-level=error src",
    "prepare": "node scripts/prepare.js",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start": "node dist/main",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test": "vitest"
  },
  "dependencies": {
    "@googleapis/sheets": "^5.0.5",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/cqrs": "^10.2.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "discord.js": "^14.14.1",
    "firebase-admin": "^12.0.0",
    "google-auth-library": "^9.6.3",
    "joi": "^17.12.2",
    "nestjs-pino": "^4.0.0",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "ts-pattern": "^5.0.8",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.0",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@commitlint/cz-commitlint": "^19.0.3",
    "@commitlint/prompt-cli": "^19.0.3",
    "@flydotio/dockerfile": "^0.5.2",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "change-case": "^5.4.3",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "ejs-lint": "^2.0.0",
    "husky": "^9.0.11",
    "hygen": "^6.2.11",
    "inquirer": "^9.2.15",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.3.1"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
