{
  "name": "ulti-project",
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e",
  "version": "1.14.1",
  "description": "Ulti-Project Discord Bot",
  "author": "",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && nest build",
    "check": "biome check src",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docker:build": "docker build . -t ulti-project-bot:latest",
    "format": "biome format . --write",
    "g:slash-command": "hygen slash-command new",
    "lint": "biome lint --diagnostic-level=error src",
    "lint:ci": "biome ci src",
    "prepare": "node scripts/prepare.js",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start": "node dist/main",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test": "vitest",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist"
  },
  "dependencies": {
    "@googleapis/sheets": "^9.0.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/cqrs": "^10.2.7",
    "@sentry/cli": "^2.33.0",
    "@sentry/node": "^8.20.0",
    "@sentry/profiling-node": "^8.20.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.12",
    "discord.js": "^14.15.3",
    "firebase-admin": "^12.3.0",
    "google-auth-library": "^9.12.0",
    "joi": "^17.13.3",
    "nestjs-pino": "^4.1.0",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "title-case": "^4.3.1",
    "ts-pattern": "^5.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/prompt-cli": "^19.3.1",
    "@flydotio/dockerfile": "^0.5.8",
    "@golevelup/ts-vitest": "^0.5.0",
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.0.4",
    "@vitest/ui": "^2.0.4",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "ejs-lint": "^2.0.0",
    "husky": "^9.1.4",
    "hygen": "^6.2.11",
    "inquirer": "^9.3.6",
    "jest-extended": "^4.0.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "unplugin-swc": "^1.5.1",
    "vitest": "^2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
