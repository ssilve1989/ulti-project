    "hygen": "^6.2.11",
    "inquirer": "^9.2.23",
    "jest-extended": "^4.0.2",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.6.0"
