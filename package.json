{
  "name": "ulti-project",
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "version": "1.40.2",
  "description": "Ulti-Project Discord Bot",
  "author": "Aeo <aeo.games14@gmail.com>",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "engines": {
    "node": "^24.11.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "check": "biome check src",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "docker:build": "docker build . -t ulti-project-bot:latest",
    "format": "biome format . --write",
    "g:slash-command": "hygen slash-command new",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "lint": "biome lint --diagnostic-level=error src",
    "lint:ci": "biome ci --diagnostic-level=error src",
    "prepare": "node scripts/prepare.js",
    "start:sentry": "NODE_OPTIONS=\"--import=./instrumentation.mjs\" pnpm start",
    "start": "dotenvx run -f .env -f .env.development -- node dist/main",
    "test": "vitest",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.0",
    "@googleapis/sheets": "^12.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/cqrs": "^11.0.3",
    "@sentry/cli": "^2.57.0",
    "@sentry/nestjs": "^10.22.0",
    "@sentry/profiling-node": "^10.22.0",
    "cron": "^4.3.3",
    "dayjs": "^1.11.18",
    "discord.js": "^14.23.2",
    "firebase-admin": "^13.5.0",
    "google-auth-library": "^10.4.1",
    "graphql-request": "^7.3.1",
    "nestjs-pino": "^4.4.1",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "title-case": "^4.3.2",
    "ts-pattern": "^5.9.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@commitlint/prompt-cli": "^20.1.0",
    "@flydotio/dockerfile": "^0.7.10",
    "@golevelup/ts-vitest": "^0.5.2",
    "@graphql-codegen/cli": "6.0.1",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript": "5.0.2",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "commitizen": "^4.3.1",
    "husky": "^9.1.7",
    "hygen": "^6.2.11",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.8",
    "vitest": "^4.0.8"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@firebase/util",
      "@nestjs/core",
      "@sentry-internal/node-cpu-profiler",
      "@sentry/cli",
      "@swc/core",
      "esbuild",
      "nestjs-pino"
    ],
    "ignoredBuiltDependencies": [
      "protobufjs"
    ]
  }
}
