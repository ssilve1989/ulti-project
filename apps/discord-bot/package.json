{
  "name": "@ulti-project/discord-bot",
  "version": "1.32.1",
  "description": "Ulti-Project Discord Bot",
  "author": "",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/main.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "dev": "nest start --watch",
    "start": "node --import ./instrumentation.mjs dist/main",
    "start:debug": "nest start --debug --watch",
    "start:dev:sentry": "NODE_OPTIONS=\"--import=./instrumentation.mjs\" nest start --watch",
    "test": "vitest",
    "test:ci": "CI=true vitest run --coverage",
    "test:cov": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint --diagnostic-level=error .",
    "g:slash-command": "hygen slash-command new",
    "graphql:codegen": "graphql-codegen --config codegen.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org ulti-project --project ulti-project-bot ./dist && sentry-cli sourcemaps upload --org ulti-project --project ulti-project-bot ./dist",
    "docker:build": "cd ../.. && docker build -f apps/discord-bot/Dockerfile -t ulti-project-bot:latest ."
  },
  "dependencies": {
    "@googleapis/sheets": "^9.8.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/platform-fastify": "^11.1.3",
    "@sentry/cli": "^2.45.0",
    "@sentry/nestjs": "^9.22.0",
    "@sentry/profiling-node": "^9.22.0",
    "@ulti-project/shared": "workspace:*",
    "cache-manager": "^6.4.3",
    "cron": "^4.3.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.19.3",
    "firebase-admin": "^13.4.0",
    "google-auth-library": "^9.15.1",
    "graphql-request": "^7.2.0",
    "nestjs-pino": "^4.4.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "title-case": "^4.3.2",
    "ts-pattern": "^5.7.1",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@golevelup/ts-vitest": "^0.5.2",
    "@graphql-codegen/cli": "5.0.6",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript": "4.1.6",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.1.4",
    "hygen": "^6.2.11",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "typescript": "^5.8.3",
    "unplugin-swc": "^1.5.3",
    "vitest": "^3.1.4"
  }
}
